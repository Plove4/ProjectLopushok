<Window x:Class="ProjectLopushok.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ProjectLopushok"
        mc:Ignorable="d"
        Title="Лопушок" Height="500" Width="850" MinWidth="850" MinHeight="500">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="60"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <Grid Background="#CEFFF9">
            <Image Source="Resources\Лопушок.png" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="10 3 0 3"></Image>
            <TextBlock Text="Лопушок" VerticalAlignment="Center" HorizontalAlignment="Center" FontSize="50" FontWeight="Black" FontFamily="Gabriola"></TextBlock>
        </Grid>
        <Frame x:Name="frmMain" Grid.Row="1" NavigationUIVisibility="Hidden"></Frame>
    </Grid>
</Window>
